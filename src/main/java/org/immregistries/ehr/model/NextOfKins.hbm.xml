<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.immregistries.ehr.model">
  <class name="NextOfKins" table="Next-of-kins">
    <id name="nextOfKinsId" column="Next-of-kins_id">
      <generator class="native"/>
    </id>
    <many-to-one name="patientId" column="patient_id" class="Patient"/>
    <property name="birthDate" column="birth_date" type="date"/>
    <property name="nameLast" column="name_last" type="string"/>
    <property name="nameFirst" column="name_first" type="string"/>
    <property name="nameMiddle" column="name_middle" type="string"/>
    <property name="motherMaiden" column="mother_maiden" type="string"/>
    <property name="sex" column="sex" type="string"/>
    <property name="race" column="race" type="string"/>
    <property name="addressLine1" column="address_line1" type="string"/>
    <property name="addressLine2" column="address_line2" type="string"/>
    <property name="addressCity" column="address_city" type="string"/>
    <property name="addressState" column="address_state" type="string"/>
    <property name="addressZip" column="address_zip" type="string"/>
    <property name="addressCountry" column="address_country" type="string"/>
    <property name="addressCountyParish" column="address_county_parish" type="string"/>
    <property name="phone" column="phone" type="string"/>
    <property name="email" column="email" type="string"/>
    <property name="ethnicity" column="ethnicity" type="string"/>
  </class>
</hibernate-mapping>