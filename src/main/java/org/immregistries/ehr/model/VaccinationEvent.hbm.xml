<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.immregistries.ehr.model">
  <class name="VaccinationEvent" table="vaccination_event">
    <id name="vaccinationEventId" column="vaccination_event_id">
      <generator class="native"/>
    </id>
    <many-to-one name="administeringClinicianId" column="clinician_id" class="Clinician"/>
    <many-to-one name="enteringClinicianId" column="clinician_id" class="Clinician"/>
    <many-to-one name="orderingClinicianId" column="clinician_id" class="Clinician"/>
    <many-to-one name="administeringFacilityId" column="facility_id" class="Facility"/>
    <many-to-one name="patientId" column="patient_id" class="Patient"/>
    <many-to-one name="vaccineId" column="vaccine_id" class="Vaccine"/>
    <many-to-one name="logId" column="log_id" class="LogsOfModifications"/>
  </class>
</hibernate-mapping>