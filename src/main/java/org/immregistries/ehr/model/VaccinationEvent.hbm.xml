<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.immregistries.ehr.model">
  <class name="VaccinationEvent" table="vaccination_event">
    <id name="vaccinationEventId" column="vaccination_event_id">
      <generator class="native"/>
    </id>
    <many-to-one name="administeringClinician" column="administering_clinician_id" class="Clinician"/>
    <many-to-one name="enteringClinician" column="entering_clinician_id" class="Clinician"/>
    <many-to-one name="orderingClinician" column="ordering_clinician_id" class="Clinician"/>
    <many-to-one name="administeringFacility" column="facility_id" class="Facility"/>
    <many-to-one name="patient" column="patient_id" class="Patient"/>
    <many-to-one name="vaccine" column="vaccine_id" class="Vaccine"/>
    <many-to-one name="log" column="log_id" class="LogsOfModifications"/>
  </class>
</hibernate-mapping>