<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.immregistries.ehr.model">
  <class name="Tester" table="tester">
    <id name="testerId" column="tester_id">
      <generator class="native"/>
    </id>
    <property name="loginUsername" column="login_username" type="string"/>
    <property name="loginPassword" column="login_password" type="string"/>
  </class>
</hibernate-mapping>