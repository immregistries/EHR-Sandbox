<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.immregistries.ehr.model">
  <class name="LogsOfModifications" table="logs_of_modifications">
    <id name="logId" column="log_id">
      <generator class="native"/>
    </id>
    <property name="modifType" column="modif_type" type="string"/>
    <property name="modifDate" column="modif_date" type="date"/>
  </class>
</hibernate-mapping>