  	<servlet-name>Silos</servlet-name>
  	<servlet-class>org.immregistries.ehr.servlet.Silos</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>IIS_message</display-name>
  	<servlet-name>IIS_message</servlet-name>
  	<servlet-class>org.immregistries.ehr.servlet.IISMessage</servlet-class>
