  </servlet>
  <servlet>
  	
  	<display-name>EntryRecord</display-name>
  	<servlet-name>entry_record</servlet-name>
  	<servlet-class>org.immregistries.ehr.servlet.EntryRecord</servlet-class>
  </servlet>
    <servlet>
    <display-name>Facility </display-name>
  <servlet-name> Facility</servlet-name>
  <servlet-class>org.immregistries.ehr.servlet.FacilityCreation</servlet-class>
