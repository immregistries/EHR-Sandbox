<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee;http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
  version="2.4">

  <display-name>EHR-Sandbox</display-name>
  
  <welcome-file-list>  
  	<welcome-file>authentication.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>home</servlet-name>
    <servlet-class>org.immregistries.ehr.servlet.HomeServlet.java</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>Entry</display-name>
  	<servlet-name>Entry</servlet-name>
  	<servlet-class>org.immregistries.ehr.servlet.Entry</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>patient_record</display-name>
  	<servlet-name>patient_record</servlet-name>
  	<servlet-class>org.immregistries.ehr.servlet.patient_record</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>Silos</display-name>
  	<servlet-name>Silos</servlet-name>
  	<servlet-class>org.immregistries.ehr.servlet.Silos</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>IIS_message</display-name>
  	<servlet-name>IIS_message</servlet-name>
  	<servlet-class>org.immregistries.ehr.servlet.IIS_message</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>Silo_creation</display-name>
  	<servlet-name>Silo_creation</servlet-name>
  	<servlet-class>org.immregistries.ehr.servlet.Silo_creation</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>patient_creation</display-name>
  	<servlet-name>patient_creation</servlet-name>
  	<servlet-class>org.immregistries.ehr.servlet.patient_creation</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>facility_patient_display</display-name>
  	<servlet-name>facility_patient_display</servlet-name>
  	<servlet-class>org.immregistries.ehr.servlet.facility_patient_display</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Entry</servlet-name>
  	<url-pattern>/Entry</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>patient_record</servlet-name>
  	<url-pattern>/patient_record</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Silos</servlet-name>
  	<url-pattern>/Silos</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>IIS_message</servlet-name>
  	<url-pattern>/IIS_message</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Silo_creation</servlet-name>
  	<url-pattern>/Silo_creation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>patient_creation</servlet-name>
  	<url-pattern>/patient_creation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>facility_patient_display</servlet-name>
  	<url-pattern>/facility_patient_display</url-pattern>
  </servlet-mapping>
</web-app>
