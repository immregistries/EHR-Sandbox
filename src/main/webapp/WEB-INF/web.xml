  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FHIR_messaging</servlet-name>
    <url-pattern>/FHIR_messaging</url-pattern>
  </servlet-mapping>

  <filter>
        <filter-name>loginFilter</filter-name>
        <filter-class>org.immregistries.ehr.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
      <filter-name>loginFilter</filter-name>
      <url-pattern>/*</url-pattern>
  </filter-mapping>
