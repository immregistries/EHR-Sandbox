<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping
	package="org.immregistries.ehr.model">
	<class name="Vaccine" table="vaccine">
		<id name="vaccineId" column="vaccine_id">
			<generator class="native" />
		</id>
		<property name="createdDate" column="created_date"
			type="date" />
		<property name="updatedDate" column="updated_date"
			type="date" />
		<property name="administeredDate" column="administered_date"
			type="date" />
		<property name="vaccineCvxCode" column="vaccine_cvx_code"
			type="string" />
		<property name="vaccineNdcCode" column="vaccine_ndc_code"
			type="string" />
		<property name="vaccineMvxCode" column="vaccine_mvx_code"
			type="string" />
		<property name="administeredAmount"
			column="administered_amount" type="string" />
		<property name="informationSource"
			column="information_source" type="string" />
		<property name="lotnumber" column="lot_number" type="string" />
		<property name="expirationDate" column="expiration_date"
			type="date" />
		<property name="completionStatus" column="completion_status"
			type="string" />
		<property name="actionCode" column="action_code"
			type="string" />
		<property name="refusalReasonCode"
			column="refusal_reason_code" type="string" />
		<property name="bodySite" column="body_site" type="string" />
		<property name="bodyRoute" column="body_route" type="string" />
		<property name="fundingSource" column="funding_source"
			type="string" />
		<property name="fundingEligibility"
			column="funding_eligibility" type="string" />
	</class>
</hibernate-mapping>